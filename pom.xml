<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.2.4.RELEASE</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <groupId>com.wlcb</groupId>
    <artifactId>wlcb</artifactId>
    <version>1.0.1-SNAPSHOT</version>

    <packaging>pom</packaging>

    <properties>
        <java.version>1.8</java.version>
        <com.wlcb.version>1.0.1-SNAPSHOT</com.wlcb.version>
    </properties>

    <modules>
        <module>wlcb-module-base</module>
        <module>wlcb-module-common</module>
        <module>wlcb-module-dbs</module>
        <module>wlcb-web-tiwen</module>
        <module>wlcb-web-corporate</module>
        <module>wlcb-web-sync</module>
        <module>wlcb-core-user</module>
    </modules>

    <dependencyManagement>
        <dependencies>

            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>druid-spring-boot-starter</artifactId>
                <version>1.1.10</version>
            </dependency>

            <dependency>
                <groupId>org.projectlombok</groupId>
                <artifactId>lombok</artifactId>
                <version>1.18.10</version>
                <scope>provided</scope>
            </dependency>

            <dependency>
                <groupId>org.springframework</groupId>
                <artifactId>spring-tx</artifactId>
                <version>5.2.3.RELEASE</version>
            </dependency>

            <dependency>
                <groupId>com.alibaba</groupId>
                <artifactId>fastjson</artifactId>
                <version>1.2.62</version>
            </dependency>

            <dependency>
                <groupId>org.apache.commons</groupId>
                <artifactId>commons-lang3</artifactId>
                <version>3.9</version>
            </dependency>

            <!--<dependency>
                <groupId>com.github.pagehelper</groupId>
                <artifactId>pagehelper</artifactId>
                <version>5.0.0</version>
            </dependency>-->
            <dependency>
                <groupId>com.github.pagehelper</groupId>
                <artifactId>pagehelper-spring-boot-starter</artifactId>
                <version>1.2.5</version>
            </dependency>

            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-module-base</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-module-dbs</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-module-common</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-core-user</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-web-sync</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-web-corporate</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wlcb</groupId>
                <artifactId>wlcb-web-tiwen</artifactId>
                <version>${com.wlcb.version}</version>
            </dependency>

        </dependencies>
    </dependencyManagement>


</project>