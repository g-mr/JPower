<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wlcb.ylth.module.dbs.dao.sync.SyncMapper">


    <insert id="insterList">
        insert into ${tableName}
        <foreach collection="listFileds" item="filed" open="(id," separator="," close=")">${filed}</foreach>
        values
        <foreach collection="listMap" item="map" separator=",">
            (
            replace(uuid(),'-',''),
            <foreach collection="listFileds" item="filed" separator=",">
                #{map.${filed}}
            </foreach>
            )
        </foreach>
    </insert>
</mapper>